{
  "name": "eosio.contracts",
  "generators": [
    {
      "name": "collate_markdown",
      "options": {
        "docs_dir": "docs"
      }
    },
    {
      "name": "mdjavadoc",
      "options": {
        "source_dirs": [
          "contracts/flon.token/include/flon.token/",
          "contracts/flon.wrap/include/flon.wrap/",
          "contracts/flon.bios/include/flon.bios/",
          "contracts/flon.system/include/flon.system/",
          "contracts/flon.msig/include/flon.msig/"
        ],
        "output_dir": "action-reference"
      }
    }
  ],
  "skip_default_filters": true,
  "filters": [
    {
      "name": "sanitize",
      "options": {
        "exclude": ["action-reference"]
      }
    },
    {
      "name": "capitalize",
      "options": {
        "mode": "all",
        "exclude": ["action-reference"]
      }
    }
  ]
}
